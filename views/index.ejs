                         <% } %>
                        <ul class="submenu">
                            <li><a href="/connection"><i class="fa fa-angle-right"></i>Se connecter<i class="fa fa-circle"></i></a></li>
                            <li><a href="/inscription"><i class="fa fa-angle-right"></i>S'inscrire<i class="fa fa-circle"></i></a></li>
                        </ul>
                    </li>
                    <% if(name ){ %>
