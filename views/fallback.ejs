    <link rel="stylesheet" href="lib/lobibox/css/lobibox.min.css">
    <link rel="stylesheet" href="lib/highlight/github.css">
    <link rel="stylesheet" href="demo/demo.css">
</head>
<body  style="background-image:url(images/To_Do_List.png); ">

    <div class="container">
        <div id="lobilist">
            <div id="lobilist-examples">
                <div>
                    <div class="bs-example"  style="background-color:rgba(0, 0, 0, 0.5); border-color:black">
                        <div id="todo-lists-basic-demo"></div>
                    </div>
                </div>
                <div>
                    <div class="bs-example"  style="background-color:rgba(0, 0, 0, 0.5);border-color:black">
                        <div id="actions-by-ajax"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <!--Default installation-->
    <script src="lib/jquery/jquery.min.js"></script>
    <script src="lib/jquery/jquery-ui.min.js"></script>
    <script src="lib/jquery/jquery.ui.touch-punch-improved.js"></script>
